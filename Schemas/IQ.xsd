<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/IQ" xmlns:tns="http://www.example.org/IQ" elementFormDefault="qualified">

    <complexType name="IQsType">
    	<sequence>
    		<element name="tests" type="tns:IQType" maxOccurs="unbounded" minOccurs="1"></element>
    	</sequence>
    </complexType>
    
    <complexType name="IQType">
    	<sequence>
    		<element name="CaseId" type="int"></element>
    	</sequence>
    </complexType>

    <complexType name="ActivityType">
    	<sequence>
    		<element name="ClientId" type="int"></element>
    		<element name="CaseId" type="int"></element>
    		<element name="ActivityType" type="string"></element>
    		<element name="ActivityCode" type="string"></element>
    		<element name="Description" type="string"></element>
    	</sequence>
    </complexType>
    
    <complexType name="ActivitiesType">
    	<sequence>
    		<element name="Activities" type="tns:ActivityType" maxOccurs="unbounded" minOccurs="1"></element>
    	</sequence>
    </complexType>

    <element name="Activities" type="tns:ActivitiesType"></element>

    <element name="IQs" type="tns:IQsType"></element>

    <element name="Activity" type="tns:ActivityType"></element>

    <element name="IQ" type="tns:IQType"></element>
</schema>